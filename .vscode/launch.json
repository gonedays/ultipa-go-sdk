  "version": "2.0.0",
  "inputs": [
    {
      "id": "regexpName",
      "type": "pickString",
      "options": [
        ".",
        "FindNode",
        "InsertNode",
        "FindEdge",
        "InsertEdge",
        "Khop",
        "AB",
        "Spread",
        "UQL",
        "GenNodesID",
        "GenEdgesID",
        "DeleteNodes",
        "DeleteEdges",
        "ListGraph"
      ],
      "description": "Select Test Item"
    }
  ],
  "configurations": [
    {
      "name": "Run Test",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "env": {},
      "program": "${workspaceFolder}/test",
      "args": ["-test.run", "${input:regexpName}", "-test.timeout", "20m"]
    }
  ]
}
